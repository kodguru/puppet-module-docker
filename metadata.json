{
  "name": "juliengk-docker",
  "version": "0.1.7",
  "author": "juliengk",
  "summary": "Manages docker",
  "license": "Apache-2.0",
  "source": "git://github.com/juliengk/puppet-module-docker.git",
  "project_page": "https://github.com/juliengk/puppet-module-docker",
